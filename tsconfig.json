{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/node16-strictest-esm/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": false,
    "skipLibCheck": true
  },
  "include": ["./src/**/*", "./gatsby-*.ts"]
}
